<?xml version="1.0"?>

<!DOCTYPE jdo PUBLIC
  "-//Sun Microsystems, Inc.//DTD Java Data Objects Metadata 1.0//EN"
  "http://java.sun.com/dtd/jdo_1_0.dtd">

<jdo>
  <package name="org.apache.maven.plugin.jpox">
    <class name="Person">
      <field name="name" default-fetch-group="true" primary-key="true"/>
      <field name="age" default-fetch-group="true"/>
    </class>
  </package>
</jdo>
