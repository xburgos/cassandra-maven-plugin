<project>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>xmlbeans-mojo-54</artifactId>
  <groupId>org.codehaus.mojo</groupId>
  <version>1.0</version>
  <packaging>jar</packaging>
  <name>Test Case for MOJO-54</name>
  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>xmlbeans-maven-plugin</artifactId>
        <version>1.0-SNAPSHOT</version>
        <executions>
           <execution>
              <goals>
                 <goal>xmlbeans</goal>
              </goals>
           </execution>
        </executions>
        <inherited>true</inherited>
        <configuration>
           <xmlConfigs>
              <xmlConfig implementation="java.io.File">src/xsdconfig</xmlConfig>
              <xmlConfig implementation="java.io.File">src/xsdconfig2/explicit.xsdconfig</xmlConfig>
           </xmlConfigs>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
