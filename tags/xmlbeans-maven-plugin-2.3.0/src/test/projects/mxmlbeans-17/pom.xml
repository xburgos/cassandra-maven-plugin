<project>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>xmlbeans-mxmlbeans-17</artifactId>
  <groupId>org.codehaus.mojo</groupId>
  <version>1.0</version>
  <packaging>jar</packaging>
  <name>Test Case for MXMLBEANS-17</name>
  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>xmlbeans-maven-plugin</artifactId>
        <executions>
           <execution>
              <goals>
                 <goal>xmlbeans</goal>
              </goals>
           </execution>
        </executions>
        <configuration>
           <outputJar>${project.build.directory}/libs/result.jar</outputJar>
        </configuration>
      </plugin>
    </plugins>
  </build>

</project>
