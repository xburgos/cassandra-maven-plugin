<?xml version="1.0"?>

<model>
  <id>pde-product-descriptor</id>
  <name>Product</name>
  <description>
    <![CDATA[
    <p>
An Eclipse "*.product" file.
    </p>
    ]]>
  </description>
  <defaults>
    <default>
      <key>package</key>
      <value>org.codehaus.mojo.pde.descriptor</value>
    </default>
  </defaults>
  <classes>
    <class>
      <name>Descriptor</name>
      <version>1.0.0</version>
      <fields>
          <field xml.attribute="true">
          <name>id</name>
          <required>true</required>
          <type>String</type>
          <required>true</required>
        </field>
        <field xml.attribute="true">
          <name>name</name>
          <required>true</required>
          <type>String</type>
        </field>
        <field xml.attribute="true">
          <name>version</name>
          <required>false</required>
          <type>String</type>
        </field>        
      </fields>
    </class>
  
    <class rootElement="true" xml.tagName="product">
      <name>ProductDescriptor</name>
      <superClass>Descriptor</superClass>
      <description>
        PDE *.product Java binding.
      </description>
      <fields>
        <field xml.attribute="true">
          <name>id</name>
          <required>true</required>
          <type>String</type>
          <required>true</required>
        </field>
        <field xml.attribute="true">
          <name>name</name>
          <required>true</required>
          <type>String</type>
        </field>
        <field xml.attribute="true">
          <name>version</name>
          <required>true</required>
          <type>String</type>
        </field>
      </fields>      
    </class>
  </classes>
</model>

