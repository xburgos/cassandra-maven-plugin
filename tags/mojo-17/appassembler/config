      <mainClass>frs.framework.core.launch.LauncherWithInitialSleep</mainClass>
      <commandLineArguments>
        <commandLineArgument>-moduleName=execution</commandLineArgument>
      </commandLineArguments>
    </daemon>
  </daemons>
  <extraJvmArguments>-Dlog4j.configuration=log4j-execution.properties</extraJvmArguments>
</configuration>
