  <platforms>
    <platform>unix</platform>
    <platform>windoze</platform>>
    <platform>all</platform>
  <platforms>

  <!-- optional element, generates wrapper-script if ommited -->
  <booters>
    <booter>wrapper-script</booter>
    <booter>appassembler-booter</booter>
    <booter>jsw</booter>
    <booter>generic-xml</booter>
  </booters>
  
  <programs>
    <program>
      <id>myApp</id>
      <mainClass>com.westerngeco.example.App</mainClass>
      <jvmSettings>
        <initialMemorySize>345</initialMemorySize>
        <maxMemorySize>234</maxMemorySize>
        <maxStackSize>321</maxStackSize>
        <systemProperties>
          <systemProperty>foo=bar</systemProperty>
          <systemProperty>trygve=cool</systemProperty>
        </systemProperties>
     </jvmSettings>

     <!-- optional element, overrides global platforms element -->
     <platforms>
       <platform>unix</platform>
       <platform>windoze</platform>>
       <platform>all</platform>
     <platforms>

     <!-- optional element, overrides global booter element -->
     <booters>
       <booter>wrapper-script</booter>
       <booter>appassembler-booter</booter>
       <booter>jsw</booter>
       <booter>generic-xml</booter>
      </booters>
    </program>
  <programs>
<configuration>


<!-- til fredag -->
<configuration>
  <assembleDirectory>target/appassembler</assembleDirectory>
  <includeConfigurationDirectoryInClasspath>true</includeConfigurationDirectoryInClasspath>
  <daemons>
    <daemon>
      <platforms>
        <platform>generic</platform>
        <platform>booter-windows</platform>
      </platforms>
