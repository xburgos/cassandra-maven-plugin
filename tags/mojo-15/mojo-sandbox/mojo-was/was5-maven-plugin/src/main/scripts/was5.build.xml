			<pathelement location="${env.USERPROFILE}/.m2/repository/org/codehaus/mojo/WAS-Plugin-anttasks/1.0/WAS-Plugin-anttasks-1.0.jar" />
		</classpath>
	</taskdef>

	<taskdef name="var" classname="org.codehaus.mojo.wasanttasks.ChangePropertyTask">
		<classpath>
			<pathelement location="${env.USERPROFILE}/.m2/repository/org/codehaus/mojo/WAS-Plugin-anttasks/1.0/WAS-Plugin-anttasks-1.0.jar" />
