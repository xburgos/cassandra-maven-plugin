			<arg value="${wasnode}" />
			<arg value="${srv}" />
			<arg value="${dntwt}" />
			<arg value="${silent}" />
			<arg value="${logFile}" />
			<arg value="${logFileName}" />
			<arg value="${repllog}" />
			<arg value="${trc}" />
			<arg value="${scriptSet}" />
